FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "6.5";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 2.22045e-16;

boundaryField
{
    movingWall
    {
        type            kqRWallFunction;
        value           uniform 2.22045e-16;
    }
    fixedWalls
    {
        type            kqRWallFunction;
        value           uniform 2.22045e-16;
    }
    frontAndBack
    {
        type            empty;
    }
}


